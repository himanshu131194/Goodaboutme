<!--main content start-->
<div class="main-content">
    <div class="container">
        <div class="row">
            <div class="col-md-8">
           {{#if singlePost}}
                     {{#each singlePost}}
                        <article class="post">
                         <div class="post-thumb">
                        <a href="blog.html"><img src="/images/blog-1.jpg" alt=""></a>

                        <a href="blog.html" class="post-thumb-overlay text-center">
                            <div class="text-uppercase text-center">View Post</div>
                        </a>
                    </div>
                    <div class="post-content">
                        <header class="entry-header text-center text-uppercase">
                            <h1 class="entry-title"><a href="blog.html">{{this.postTitle}}</a></h1>
                        </header>
                        <div class="entry-content">
                            <p>
                              {{{this.postData}}}
                            </p>
                            <p></p>
                        </div>
                        <div class="social-share">
                            <span class="social-share-title pull-left text-capitalize">By Rubel On February 12, 2016</span>
                            <ul class="text-center pull-right">
                                <li><a class="s-facebook" href="home-2.html"><i class="fa fa-facebook"></i></a></li>
                                <li><a class="s-twitter" href="home-2.html"><i class="fa fa-twitter"></i></a></li>
                                <li><a class="s-google-plus" href="home-2.html"><i class="fa fa-google-plus"></i></a></li>
                                <li><a class="s-linkedin" href="home-2.html"><i class="fa fa-linkedin"></i></a></li>
                                <li><a class="s-instagram" href="home-2.html"><i class="fa fa-instagram"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    </article>
                     {{/each}}
                    {{/if}}
                </article>
